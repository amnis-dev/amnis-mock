{
  "name": "@amnis/mock",
  "version": "0.14.1",
  "description": "Local API mocking for Amnis processors.",
  "keywords": [],
  "author": "Eric Crowell <admin@amnis.dev>",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/amnis-dev/amnis-mock",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-mock"
  },
  "files": [
    "types",
    "dist"
  ],
  "types": "./types/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "build": "pnpm build:types && pnpm build:dist",
    "build:dist": "vite build --mode production",
    "build:types": "tsc -p tsconfig.build.json",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "clean": "rimraf ./dist ./lib ./types ./tsconfig.build.tsbuildinfo"
  },
  "dependencies": {
    "msw": "^1.1.0"
  },
  "peerDependencies": {
    "@amnis/state": "^0.14.5"
  },
  "devDependencies": {
    "@amnis/eslint-config-node": "^2.2.4",
    "@amnis/state": "^0.15.1",
    "@amnis/tsconfig-node": "^2.2.4",
    "@reduxjs/toolkit": "2.0.0-alpha.2",
    "@types/node": "^18.14.1",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.34.0",
    "msw": "^1.1.0",
    "rimraf": "^4.1.2",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.28.5"
  }
}
